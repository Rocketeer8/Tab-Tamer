{
    "manifest_version": 3,
    "name": "Tab Tamer - Custom Tab Limit",
    "description": "Base Level Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "tab blue alt.png"
    },
    "icons": {
      "16": "tab blue alt.png",
      "32": "tab blue alt.png",
      "48": "tab blue alt.png",
      "128": "tab blue alt.png"
    }
  }